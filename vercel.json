
{
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./web/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" },
      { "src": "/(.*)", "dest": "/web/index.html" }
    ]
  }
